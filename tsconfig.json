{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": false,
        "jsx": "react-jsx",
        "baseUrl": "src",
        "paths": {
            "app-home/*": [
                "app-home/*"
            ],
            "app/*": [
                "app/*"
            ],
            "component-library/*": [
                "component-library/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}